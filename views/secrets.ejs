<%- include('partials/header') %>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    <h1 class="display-3">Secrets our users have shared!</h1>
    <!--Display every secret that users have posted-->
    <%usersWithSecrets.forEach(function(user){ %>
    <p class="secret-text"><%=user.secret%></p>
    <% }) %>
    <hr />
    <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
    <a class="btn btn-dark btn-lg" href="/submit" role="button"
      >Submit a Secret</a
    >
  </div>
</div>

<%- include('partials/footer') %>
